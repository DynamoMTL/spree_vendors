= render :partial => 'spree/shared/error_messages', :locals => { :target => @vendor }
= f.input :name
= f.input :description
= f.input :available_at
= f.input :logo, as: :file

= f.semantic_fields_for :vendor_images do |vendor_image|
  - if vendor_image.object.new_record?
    = vendor_image.input :image, as: :file
